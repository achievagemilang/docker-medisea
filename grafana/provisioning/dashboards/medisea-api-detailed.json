{
  "title": "Medisea API - Detailed Metrics",
  "tags": [
    "medisea",
    "api",
    "production",
    "detailed"
  ],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "id": 1,
      "title": "Request Rate by Route",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum by (route) (rate(otel_http_server_requests_total[5m]))",
          "legendFormat": "{{route}}",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "Prometheus"
          }
        }
      ],
      "yaxes": [
        {
          "format": "reqps",
          "label": "Requests/sec"
        },
        {
          "format": "short"
        }
      ],
      "xaxis": {
        "mode": "time"
      }
    },
    {
      "id": 2,
      "title": "Request Duration by Percentile",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(otel_http_server_request_duration_seconds_bucket[5m]))",
          "legendFormat": "p50",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "Prometheus"
          }
        },
        {
          "expr": "histogram_quantile(0.90, rate(otel_http_server_request_duration_seconds_bucket[5m]))",
          "legendFormat": "p90",
          "refId": "B",
          "datasource": {
            "type": "prometheus",
            "uid": "Prometheus"
          }
        },
        {
          "expr": "histogram_quantile(0.95, rate(otel_http_server_request_duration_seconds_bucket[5m]))",
          "legendFormat": "p95",
          "refId": "C",
          "datasource": {
            "type": "prometheus",
            "uid": "Prometheus"
          }
        },
        {
          "expr": "histogram_quantile(0.99, rate(otel_http_server_request_duration_seconds_bucket[5m]))",
          "legendFormat": "p99",
          "refId": "D",
          "datasource": {
            "type": "prometheus",
            "uid": "Prometheus"
          }
        }
      ],
      "yaxes": [
        {
          "format": "s",
          "label": "Duration"
        },
        {
          "format": "short"
        }
      ],
      "xaxis": {
        "mode": "time"
      }
    },
