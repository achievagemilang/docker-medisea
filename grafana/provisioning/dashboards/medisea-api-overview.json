{
  "title": "Medisea API - Overview",
  "tags": [
    "medisea",
    "api",
    "production"
  ],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "id": 1,
      "title": "Request Rate",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "rate(otel_http_server_requests_total[5m])",
          "legendFormat": "{{method}} {{route}}",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "Prometheus"
          }
        }
      ],
      "yaxes": [
        {
          "format": "reqps",
          "label": "Requests/sec"
        },
        {
          "format": "short"
        }
      ],
      "xaxis": {
        "mode": "time"
      }
    },
    {
      "id": 2,
      "title": "Request Duration (p95)",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(otel_http_server_request_duration_seconds_bucket[5m]))",
          "legendFormat": "{{route}}",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "Prometheus"
          }
        }
      ],
      "yaxes": [
        {
          "format": "s",
          "label": "Duration"
        },
        {
          "format": "short"
        }
      ],
      "xaxis": {
        "mode": "time"
      }
    },
    {
      "id": 3,
      "title": "Error Rate",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "expr": "rate(otel_http_server_requests_total{status=~\"5..\"}[5m])",
          "legendFormat": "{{route}} - {{status}}",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "Prometheus"
          }
        }
      ],
      "yaxes": [
        {
          "format": "reqps",
          "label": "Errors/sec"
        },
        {
          "format": "short"
        }
      ],
      "xaxis": {
        "mode": "time"
      }
    },
    {
      "id": 4,
      "title": "In-Flight Requests",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "targets": [
        {
          "expr": "otel_http_server_in_flight_requests",
          "legendFormat": "{{method}}",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "Prometheus"
          }
        }
      ],
      "yaxes": [
        {
          "format": "short",
          "label": "Requests"
        },
        {
          "format": "short"
        }
      ],
      "xaxis": {
        "mode": "time"
      }
    },
    {
      "id": 5,
      "title": "HTTP Status Codes",
      "type": "piechart",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 16
      },
      "targets": [
        {
          "expr": "sum by (status) (rate(otel_http_server_requests_total[5m]))",
          "legendFormat": "{{status}}",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "Prometheus"
          }
        }
      ]
    },
    {
      "id": 6,
      "title": "Top Routes by Request Rate",
      "type": "table",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 16
      },
      "targets": [
        {
          "expr": "topk(10, sum by (route) (rate(otel_http_server_requests_total[5m])))",
          "format": "table",
          "instant": true,
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "Prometheus"
          }
        }
      ],
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true
            },
            "indexByName": {},
            "renameByName": {
              "route": "Route",
              "Value": "Req/sec"
            }
          }
        }
      ]
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": [
      "5s",
      "10s",
      "30s",
      "1m",
      "5m",
      "15m",
      "30m",
      "1h"
    ]
  },
  "id": null,
  "uid": "medisea-api-overview"
}