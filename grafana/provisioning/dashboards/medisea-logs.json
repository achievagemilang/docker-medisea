{
  "title": "Medisea Logs - Application Logs",
  "tags": [
    "medisea",
    "logs",
    "loki",
    "production"
  ],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "id": 1,
      "title": "Log Explorer",
      "type": "logs",
      "gridPos": {
        "h": 12,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "{service_name=\"medisea-service\"}",
          "refId": "A",
          "datasource": {
            "type": "loki",
            "uid": "Loki"
          }
        }
      ],
      "options": {
        "dedupStrategy": "none",
        "enableLogDetails": true,
        "prettifyLogMessage": false,
        "showCommonLabels": false,
        "showLabels": false,
        "showTime": true,
        "sortOrder": "Descending",
        "wrapLogMessage": false
      }
    },
    {
      "id": 2,
      "title": "Error Logs",
      "type": "logs",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 12
      },
      "targets": [
        {
          "expr": "{service_name=\"medisea-service\"} |= \"error\" | |= \"ERROR\" | |= \"Error\"",
          "refId": "A",
          "datasource": {
            "type": "loki",
            "uid": "Loki"
          }
        }
      ],
      "options": {
        "dedupStrategy": "none",
        "enableLogDetails": true,
        "prettifyLogMessage": false,
        "showCommonLabels": false,
        "showLabels": false,
        "showTime": true,
        "sortOrder": "Descending",
        "wrapLogMessage": false
      }
    },
    {
      "id": 3,
      "title": "Log Volume by Container",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 12
      },
      "targets": [
        {
          "expr": "sum by (container_name) (rate({service_name=\"medisea-service\"}[5m]))",
          "legendFormat": "{{container_name}}",
          "refId": "A",
          "datasource": {
            "type": "loki",
            "uid": "Loki"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "logs/sec",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          }
        }
      },
      "yaxes": [
        {
          "format": "short"
        },
        {
          "format": "short"
        }
      ]
    },
    {
      "id": 4,
      "title": "Log Volume by Log Level",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 20
      },
      "targets": [
        {
          "expr": "sum by (level) (rate({service_name=\"medisea-service\"} | json [5m]))",
          "legendFormat": "{{level}}",
          "refId": "A",
          "datasource": {
            "type": "loki",
            "uid": "Loki"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "logs/sec",
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 50
          }
        }
      },
      "yaxes": [
        {
          "format": "short"
        },
        {
          "format": "short"
        }
      ]
    },
    {
      "id": 5,
      "title": "Recent Errors",
      "type": "table",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 20
      },
      "targets": [
        {
          "expr": "{service_name=\"medisea-service\"} |= \"error\" | |= \"ERROR\" | |= \"Error\"",
          "refId": "A",
          "datasource": {
            "type": "loki",
            "uid": "Loki"
          },
          "format": "table",
          "instant": true
        }
      ],
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {},
            "indexByName": {},
            "renameByName": {
              "Time": "Time",
              "container_name": "Container",
              "Line": "Message"
            }
          }
        }
      ]
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": [
      "5s",
      "10s",
      "30s",
      "1m",
      "5m",
      "15m",
      "30m",
      "1h"
    ]
  },
  "id": null,
  "uid": "medisea-logs"
}