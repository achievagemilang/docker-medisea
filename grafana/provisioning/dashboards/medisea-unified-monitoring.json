{
  "title": "Medisea Unified Monitoring",
  "tags": ["medisea", "unified", "production", "overview"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "panels": [
    {
      "id": 1,
      "title": "Request Rate",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(rate(otel_http_server_requests_total[5m]))",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "Prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "green"
              },
              {
                "value": 100,
                "color": "yellow"
              },
              {
                "value": 500,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 2,
      "title": "Error Rate",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(rate(otel_http_server_requests_total{status=~\"[45]..\"}[5m]))",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "Prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "green"
              },
              {
                "value": 1,
                "color": "yellow"
              },
              {
                "value": 10,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 3,
      "title": "P95 Latency",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(otel_http_server_request_duration_seconds_bucket[5m])) by (le))",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "Prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "green"
              },
              {
                "value": 0.5,
                "color": "yellow"
              },
              {
                "value": 1,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 4,
      "title": "Log Volume",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 18,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(rate({service_name=\"medisea-service\"}[5m]))",
          "refId": "A",
          "datasource": {
            "type": "loki",
            "uid": "Loki"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "logs/sec",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "green"
              },
              {
                "value": 10,
                "color": "yellow"
              },
              {
                "value": 50,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 5,
      "title": "API Metrics",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 4
      },
      "targets": [
        {
          "expr": "sum(rate(otel_http_server_requests_total[5m]))",
          "legendFormat": "Request Rate",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "Prometheus"
          }
        },
        {
          "expr": "sum(rate(otel_http_server_requests_total{status=~\"[45]..\"}[5m]))",
          "legendFormat": "Error Rate",
          "refId": "B",
          "datasource": {
            "type": "prometheus",
            "uid": "Prometheus"
          }
        }
      ],
      "yaxes": [
        {
          "format": "reqps"
        },
        {
          "format": "short"
        }
      ]
    },
    {
      "id": 6,
      "title": "Recent Logs",
      "type": "logs",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 4
      },
      "targets": [
        {
          "expr": "{service_name=\"medisea-service\"}",
          "refId": "A",
          "datasource": {
            "type": "loki",
            "uid": "Loki"
          }
        }
      ],
      "options": {
        "dedupStrategy": "none",
        "enableLogDetails": true,
        "prettifyLogMessage": false,
        "showCommonLabels": false,
        "showLabels": false,
        "showTime": true,
        "sortOrder": "Descending",
        "wrapLogMessage": false
      }
    },
    {
      "id": 7,
      "title": "Trace Search",
      "type": "jaeger",
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 12
      },
      "targets": [
        {
          "query": "medisea-api",
          "refId": "A",
          "datasource": {
            "type": "jaeger",
            "uid": "Jaeger"
          },
          "serviceName": "medisea-api"
        }
      ],
      "options": {
        "showTraceId": true,
        "showStartTime": true,
        "showDuration": true,
        "showServiceName": true,
        "showOperationName": true
      }
    },
    {
      "id": 8,
      "title": "Error Logs",
      "type": "logs",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 22
      },
      "targets": [
        {
          "expr": "{service_name=\"medisea-service\"} |= \"error\" | |= \"ERROR\" | |= \"Error\"",
          "refId": "A",
          "datasource": {
            "type": "loki",
            "uid": "Loki"
          }
        }
      ],
      "options": {
        "dedupStrategy": "none",
        "enableLogDetails": true,
        "prettifyLogMessage": false,
        "showCommonLabels": false,
        "showLabels": false,
        "showTime": true,
        "sortOrder": "Descending",
        "wrapLogMessage": false
      }
    },
    {
      "id": 9,
      "title": "Service Map",
      "type": "nodeGraph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 22
      },
      "datasource": {
        "type": "jaeger",
        "uid": "Jaeger"
      },
      "targets": [
        {
          "query": "medisea-api",
          "refId": "A",
          "datasource": {
            "type": "jaeger",
            "uid": "Jaeger"
          },
          "serviceName": "medisea-api"
        }
      ]
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h"]
  },
  "id": null,
  "uid": "medisea-unified-monitoring"
}
